{
  "functions": {
    "api/*.js": {
      "runtime": "nodejs20.x"
    }
  },
  "rewrites": [
    { "source": "/api/:match*", "destination": "/api/:match*" },
    { "source": "/", "destination": "/Frontend/index.html" },
    { "source": "/contact.html", "destination": "/Frontend/contact.html" },
    { "source": "/companyList.html", "destination": "/Frontend/companyList.html" },
    { "source": "/:path*", "destination": "/Frontend/:path*" }
  ]
}


